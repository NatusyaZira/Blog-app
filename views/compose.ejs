<%- include("partials/header.ejs") -%>
<h1>Compose</h1>
<form action="/compose" method="post" autocomplete="off">
<div class="mb-3">
  <label for="postTitle" class="form-label">Title</label><br>
  <input type="text" class="form-control" name="postTitle">
</div>
<div class="mb-3">
  <label for="post" class="form-label">Post</label><br>
  <textarea name="postBody" class="form-control"  rows="8" cols="80"></textarea>
</div>
<br>
  <button type="submit" class="btn btn-primary" name="post">Publish</button>
</form>
<%- include("partials/footer.ejs") -%>
